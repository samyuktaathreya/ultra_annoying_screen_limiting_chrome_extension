{
  "manifest_version": 3,
  "name": "YouTube Blocker",
  "version": "1.0",
  "description": "Blocks access to YouTube.",
  "permissions": ["tabs", "storage", "webNavigation"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "IMG_1995.png"
  },
  "options_page": "options.html"
}
