{
  "manifest_version": 3,
  "name": "YouTube Blocker",
  "version": "1.0",
  "description": "Blocks access to YouTube.",
  "permissions": ["tabs", "webNavigation"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "*://*.youtube.com/*"
  ],
  "action": {
    "default_icon": "IMG_1995.png"
  }
}
